const ll = {
    val: 5,
    next: {
        val: 6,
        next: {
            val:7,
            next: {
              val: 8,
              next: null
            }
        }
    }
}
function reverseLinkedList(head){
    let prev = null;
    let next = null;
    let count = 0

    while(head !== null){
        next = head.next;
        head.next = prev;
        prev = head;
        head = next;
        count++;
    }

return count;
}
console.log(reverseLinkedList(ll))  // o/p: 4
