var a = [1, 2, 3, 6, 4, 5 ];  
var b = [ 2, 4, 3, 1, 0 ]; 

function findMissingNumbers(a,b){
	let hashMap = [];
  for(let i = 0; i<b.length; i++){
  	hashMap.push(b[i])
  }
  
  for(let i=0; i<a.length; i++){
  	if(!hashMap.includes(a[i])){
    	console.log(a[i], '====numbers====')
    }
  }
}

findMissingNumbers(a,b) 

//o/p: 6
       5
